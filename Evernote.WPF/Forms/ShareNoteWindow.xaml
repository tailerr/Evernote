<Window x:Class="Evernote.WPF.Forms.ShareNoteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Evernote.WPF.Forms"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Поделиться заметкой" Height="210" Width="310">
    <Grid>
        <Button Name="btnOK" Content="Ok" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="50" Height="20" Margin="0,0,75,10" IsDefault="True" Click="btnOK_Click"></Button>
        <Button Name="btnCancel" Content="Отмена" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="50" Height="20" Margin="0,0,10,10" IsCancel="True"></Button>
        <Label Content="Имя пользователя" Height="30" Margin="0,34,172,107" RenderTransformOrigin="0.335,0.481" />
        <TextBox Name="UserName" Height="30" Margin="127,33,23,106"/>
    </Grid>
</Window>
